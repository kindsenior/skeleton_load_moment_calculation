PROTO ForkLink [
 exposedField    SFFloat    radius          0.04
 exposedField    SFFloat    height          0.1
 exposedField    SFFloat    thickness       0.02
 exposedField    SFVec3f    sideBoxSize     0.06 0.02 0.1 # width thickness height
 exposedField    SFVec3f    bottomBoxSize   0.06 0.14 0.02 # width length thickness
 exposedField    SFVec3f    translation     0 0 0
 exposedField    SFRotation rotation        0 0 1 0
 exposedField    SFColor    diffuseColor    1 0 0 # red
 exposedField    SFColor    specularColor   1 1 1 # mirror white
 exposedField    MFNode     children        [ ]
]
{
   Transform {
      translation IS translation
      rotation IS rotation
      children [
         Transform {
            translation 0 0.06 0
            rotation 1 0 0 0
            children [
               DEF Cylinder0 Shape {
                  geometry Cylinder {
                     radius IS radius
                     height IS thickness
                  }
                  appearance Appearance {
                     material Material {
                        diffuseColor IS diffuseColor
                        specularColor IS specularColor
                     }
                  }
               }
               Transform {
                  translation 0 0 -0.05
                  children [
                     DEF Box0 Shape {
                        geometry Box {
                           size IS sideBoxSize
                        }
                        appearance Appearance {
                           material Material {
                              diffuseColor IS diffuseColor
                              specularColor IS specularColor
                           }
                        }
                     }
                  ]
               }
            ]
         }
         Transform {
            translation 0 -0.06 0
            rotation 1 0 0 0
            children [
               DEF Cylinder1 Shape {
                  geometry Cylinder {
                     radius IS radius
                     height IS thickness
                  }
                  appearance Appearance {
                     material Material {
                        diffuseColor IS diffuseColor
                        specularColor IS specularColor
                     }
                  }
               }
               Transform {
                  translation 0 0 -0.05
                  children [
                     DEF Box1 Shape {
                        geometry Box {
                           size IS sideBoxSize
                        }
                        appearance Appearance {
                           material Material {
                              diffuseColor IS diffuseColor
                              specularColor IS specularColor
                           }
                        }
                     }
                  ]
               }
            ]
         }
         Transform {
            translation 0 0 -0.09
            children [
               DEF BottomBox Shape { # bottom
                  geometry Box {
                     size IS bottomBoxSize
                  }
                  appearance Appearance {
                     material Material {
                        diffuseColor IS diffuseColor
                        specularColor IS specularColor
                     }
                  }
               }
            ]
         }
      ]
   }
}

